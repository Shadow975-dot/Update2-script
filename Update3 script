local WalkspeedTab = Window:CreateTab("üö∂Walkspeed", nil) -- Title, Image

WalkspeedTab:CreateSlider({
      Name = "Adjust Speed",
      Range = {16, 500},
      Increment = 1,
      Suffix = "Speed",
      CurrentValue = 16,
      Callback = function(Value)
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
      end,
   })

local JumpPowerTab = Window:CreateTab("ü¶∂JumpPower", nil) -- Title, Image

JumpPowerTab:CreateSlider{
   Name = "Adjust JumpPower",
   Range = {50, 500},
   Increment = 1,
   Suffix = "JumpPower",
   CurrentValue = 50,
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
   end,
}
