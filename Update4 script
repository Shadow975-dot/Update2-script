local WalkspeedTab = Window:CreateTab("🚶Walkspeed", nil) -- Title, Image

WalkspeedTab:CreateSlider({
      Name = "Adjust Speed",
      Range = {16, 500},
      Increment = 1,
      Suffix = "Speed",
      CurrentValue = 16,
      Callback = function(Value)
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
      end,
   })

